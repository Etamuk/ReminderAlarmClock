<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema id="org.gnome.shell.extensions.reminderalarmclock" path="/org/gnome/shell/extensions/reminderalarmclock/">
    <key name="drop-seconds" type="b">
      <default>false</default>
    </key>
    <key name="message" type="s">
      <default>"Knock knock"</default>
    </key>
    <key name="play-sound" type="b">
      <default>false</default>
    </key>
     <key name="sound-file-path" type="s">
      <default>"file:///usr/share/sounds/gnome/default/alerts/glass.ogg"</default>
    </key>
  </schema>
</schemalist>
